<!--pages/w_my_car/w_my_car.wxml-->
<view wx:if='{{cars_number==""}}'>
  <text class="no-car-info">这里还没有车牌，点击立刻绑定</text>
  <view class='add-car-info' bindtap='addPlatenumber'>
    <image src='/images/add_car.png'></image>
    <text>添加新车牌</text>
  </view>
</view>
<view wx:else>
  <view class='list-car'>
    <text class='list-car-number'>苏E 05E68</text>
    <text class='list-car-time'>2018-06-26</text>
    <view class='list-pay'>
      <text>去支付</text>
    </view>
  </view>
  <view class='add-car-info' bindtap='addPlatenumber' wx:if="{{cars_number.length<5}}">
    <image src='/images/add_car.png'></image>
    <text>添加新车牌</text>
  </view>
</view>
